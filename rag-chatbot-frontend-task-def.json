{
  "family": "rag-chatbot-frontend-task-def",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::835156586196:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "containerDefinitions": [
    {
      "name": "rag-chatbot-frontend-container",
      "image": "835156586196.dkr.ecr.us-east-1.amazonaws.com/rag-chatbot-frontend:PLACEHOLDER_TAG",
      "portMappings": [
        { "containerPort": 8501, "protocol": "tcp" }
      ],
      "essential": true,
      "environment": [
        { "name": "AWS_DEFAULT_REGION", "value": "us-east-1" },
        { "name": "FASTAPI_URL", "value": "http://rag-chatbot-backend-service:8000" }
      ]
    }
  ]
}
