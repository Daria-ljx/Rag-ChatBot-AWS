{
  "family": "rag-chatbot-frontend-task-def",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::835156586196:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::835156586196:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "rag-chatbot-frontend-container",
      "image": "REPLACE_ME_FRONTEND",
      "portMappings": [{"containerPort": 8501, "protocol": "tcp"}],
      "essential": true,
      "environment": [
        {
          "name": "FASTAPI_URL",
          "value": "REPLACE_BACKEND_ALB"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/rag-chatbot-frontend-task-def",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "cpu": 256,
      "memory": 512
    }
  ]
}
